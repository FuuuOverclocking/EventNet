{
  "name": "eventnet",
  "version": "0.1.4",
  "main": "./index.js",
  "module": "./esm5/index.js",
  "es2015": "./esm2015/index.js",
  "typings": "./index.d.js",
  "sideEffects": false,
  "description": "Not just about flow design",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xyzingh/EventNet.git"
  },
  "keywords": [
    "eventnet",
    "flowchart",
    "debugger",
    "debugger-visualizer",
    "javascript-tools",
    "visual",
    "visual-programming"
  ],
  "author": "X.Y.Z.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xyzingh/EventNet/issues"
  },
  "homepage": "https://eventnet.justlog.xyz/",
  "scripts": {
    "build": "gulp build",
    "clean": "gulp clean",
    "other-cmds": "gulp --tasks",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "cz": "git-cz",
    "lint": "tslint --fix -c tslint.json 'src/**/*.ts'",
    "test": "gulp ts:cjs && jest",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "@types/ws": "^5.1.2",
    "tslib": "^1.9.3",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "commitizen": "^2.10.1",
    "conventional-changelog-cli": "^2.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-replace": "^1.0.0",
    "jest": "^23.0.1",
    "lodash": "^4.17.10",
    "rollup": "^0.65.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^3.0.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/lib/**/*.{js,ts}",
      "!<rootDir>/lib/**/*.d.ts",
      "!**/node_modules/**"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
