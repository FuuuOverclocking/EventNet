{
   "name": "eventnet",
   "version": "0.5.0",
   "main": "./index.js",
   "module": "./esm/index.js",
   "es2015": "./esm/index.js",
   "jsnext:main": "./esm/index.js",
   "typings": "./typings/index.d.ts",
   "sideEffects": false,
   "description": "Not just about flow design",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/xyzingh/EventNet.git"
   },
   "keywords": [
      "eventnet",
      "fbp",
      "flow-based-programming",
      "debugger",
      "debugger-visualizer",
      "javascript-tools",
      "visual",
      "visual-programming"
   ],
   "author": "X.Y.Z.",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/xyzingh/EventNet/issues"
   },
   "homepage": "https://eventnet.justlog.xyz/",
   "scripts": {
      "other-cmds": "gulp --tasks",
      "build": "gulp build",
      "clean": "gulp clean",
      "test": "jest",
      "cover": "jest -c ./jest.cover.json",
      "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
      "cz": "git-cz",
      "lint": "tslint --fix -c tslint.json 'src/**/*.ts'",
      "publish": "cd ./release/npm-runtime && npm publish",
      "version": "npm run changelog && git add CHANGELOG.md"
   },
   "dependencies": {
      "@types/ws": "^6.0.1",
      "tslib": "^1.9.3",
      "ws": "^6.1.2"
   },
   "devDependencies": {
      "@types/gulp": "^4.0.6",
      "@types/jest": "^23.3.12",
      "@types/node": "^10.1.2",
      "commitizen": "^3.0.5",
      "conventional-changelog-cli": "^2.0.1",
      "cz-conventional-changelog": "^2.1.0",
      "del": "^3.0.0",
      "gulp": "^4.0.0",
      "gulp-rename": "^1.4.0",
      "gulp-replace": "^1.0.0",
      "jest": "^23.0.1",
      "lodash": "^4.17.10",
      "rollup": "^1.1.0",
      "rollup-plugin-node-resolve": "^4.0.0",
      "rollup-plugin-replace": "^2.0.0",
      "rollup-plugin-sourcemaps": "^0.4.2",
      "ts-jest": "^23.10.5",
      "ts-node": "^8.2.0",
      "tslint": "^5.10.0",
      "typescript": "^3.2.2"
   },
   "jest": {
      "globals": {
         "ts-jest": {
            "skipBabel": true
         }
      },
      "moduleFileExtensions": [
         "ts",
         "js",
         "json"
      ],
      "notify": true,
      "transform": {
         "^.+\\.ts$": "ts-jest"
      },
      "testRegex": "/tests/.*\\.(js|ts)$",
      "testEnvironment": "node"
   },
   "config": {
      "commitizen": {
         "path": "./node_modules/cz-conventional-changelog"
      }
   }
}
